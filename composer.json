{
    "require": {
        "php": "^8.2",
        "aryehraber/statamic-captcha": "^1.9.1",
        "blade-ui-kit/blade-heroicons": "^2.0.5",
        "blade-ui-kit/blade-ui-kit": "^0.3.4",
        "brunocfalcao/blade-feather-icons": "^2.1",
        "doctrine/dbal": "^3.5.2",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/fortify": "^1.15.0",
        "laravel/framework": "^9.46.0",
        "laravel/sanctum": "^3.1.0",
        "laravel/telescope": "^4.11.0",
        "laravel/tinker": "^2.7.3",
        "league/commonmark": "^2.3.8",
        "livewire/livewire": "^2.10.8",
        "maennchen/zipstream-php": "^2.4.0",
        "opcodesio/log-viewer": "^1.7.2",
        "ryangjchandler/laravel-cloudflare-turnstile": "^0.2.0",
        "spatie/laravel-markdown": "^2.2.5",
        "spatie/schema-org": "^3.13",
        "statamic/cms": "^3.3.63",
        "statamic/eloquent-driver": "^1.1.2",
        "ublabs/blade-simple-icons": "^0.60.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.3",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.4.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pre-update-cmd": [
            "Statamic\\Console\\Composer\\Scripts::preUpdateCmd"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan statamic:install --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-install-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
